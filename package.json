{
  "scripts": {
    "build": "rollup -c rollup.config.js --watch",
    "serve-preact": "live-server --no-browser --port=8080 --entry-file=preact.html ./",
    "serve-react": "live-server --no-browser --port=8080 --entry-file=react.html ./",
    "preact": "npm-run-all -p build serve-preact",
    "react": "npm-run-all -p build serve-react"
  },
  "dependencies": {
    "preact": "^10.0.0-rc.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^4.4.10",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-react-display-name": "^7.2.0",
    "@babel/preset-env": "^7.5.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "live-server": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.17.0",
    "rollup-plugin-alias": "^1.5.2",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-includepaths": "^0.2.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-scss": "^1.0.1"
  }
}
